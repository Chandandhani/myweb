
pipeline {
    agent any
    
    environment {
        PATH = "/opt/maven3/bin:$PATH"
		}

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('git checkout'){
            steps {
                git credentialsId: 'git-creds', url: 'https://github.com/javahometech/myweb.git'
            }
        }
        stage('Maven build'){
            steps {
                sh "mvn clean package"
            }
       }
    }
}

